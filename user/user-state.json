{
  "core": {
    "tailscale_enabled": true,
    "zram_enabled": true,
    "authorized_keys": [],
    "boot": {
      "grub_device": "/dev/sda"
    },
    "security": {
      "bootstrap_mode": true
    }
  },
  "interface": {
    "terminal": {
      "shell": "zsh",
      "multiplexer": "zellij"
    },
    "theme": "catppuccin-mocha"
  },
  "languages": {
    "rust": true,
    "go": true,
    "nodejs": true,
    "python": true,
    "ruby": true
  },
  "dev_tools": {
    "docker": true
  },
  "services": {
    "postgresql": true,
    "redis": true
  },
  "frameworks": {
    "rails": true,
    "django": true,
    "nextjs": true
  },
  "editors": {
    "neovim": true,
    "helix": true,
    "vscode_server": false,
    "default": "neovim",
    "distributions": {
      "nvim": {
        "lazyvim": false,
        "astronvim": true,
        "nvchad": false,
        "lunarvim": false
      }
    }
  }
}