{
  "core": {
    "tailscale_enabled": true,
    "zram_enabled": true,
    "authorized_keys": [
      "ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAI..."
    ],
    "boot": {
      "grub_device": "/dev/vda"
    }
  },
  "interface": {
    "terminal": {
      "shell": "zsh",
      "multiplexer": "zellij"
    },
    "theme": "catppuccin-mocha"
  },
  "languages": {
    "rust": false,
    "go": false,
    "nodejs": false,
    "python": false,
    "ruby": true
  },
  "dev_tools": {
    "docker": true
  },
  "services": {
    "postgresql": true,
    "redis": false
  },
  "frameworks": {
    "rails": true,
    "django": false,
    "nextjs": false
  },
  "editors": {
    "neovim": true,
    "helix": false,
    "vscode_server": false,
    "default": "neovim",
    "distributions": {
      "nvim": {
        "lazyvim": false,
        "astronvim": true,
        "nvchad": false,
        "lunarvim": false
      }
    }
  }
}